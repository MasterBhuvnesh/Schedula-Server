{
  "name": "schedula-server",
  "version": "1.0.1",
  "description": "High-performance event management backend service with QR code generation, health monitoring, and comprehensive logging capabilities",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run format && npm run typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "qr-code",
    "express",
    "typescript",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "colors": "1.4.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "winston-daily-rotate-file": "5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "^20.10.4",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cross-env": "7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "helmet": "8.1.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "6.3.4",
    "ts-jest": "29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "winston": "3.17.0"
  }
}
